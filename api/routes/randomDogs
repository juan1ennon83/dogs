var myHeaders = new Headers();
myHeaders.append("Content-Type", "application/json");
myHeaders.append("x-api-key", "{{live_aesfZ1Mb9tdF1fVEkhyeKhnHIg3LO855ocXGqgy5Qyw9H8HueO6ezgxFeKJj25gW}}");

var requestOptions = {
  method: 'GET',
  headers: myHeaders,
  redirect: 'follow'
};

var promise = Promise.race([
  fetch('{{https://thedogapi.com}}v1/images/search?format=json&limit=15', requestOptions) // https://thedogapi.com/v1
    .then(response => response.text()),
  new Promise((resolve, reject) =>
    setTimeout(() => reject(new Error('Timeout')), 40)
  )
]);

promise.then(result => console.log(result)),
promise.catch(error => console.log(error));